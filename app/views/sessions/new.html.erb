<h1>Sign In</h1>

<%= form_with url:session_path do |form| %>
<div>
    <%= form.label :email%>
    <%= form.text_field :email, class:"form-control"%>
</div>
<div>
    <%= form.label :password%>
    <%= form.text_field :password, class:"form-control", type:"password"%>
</div>
<%= form.submit "Sign In"%>
<% end %>